{
  "name": "backend-diary",
  "version": "1.0.0",
  "description": "Backend para un diario en express.js",
  "main": "index.js",
  "scripts": {
    "development": "nodemon index.js",
    "production": "node index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo"
  },
  "keywords": [
    "javascript",
    "expre.js",
    "postgresql",
    "docker"
  ],
  "author": "Edwin Garcia Quiterio <edwinegq@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "nodemon": "^2.0.22",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8"
  }
}
